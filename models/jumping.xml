<mujoco>
    <compiler angle = "degree"/>
    <option>
        <flag gravity = "enable" contact = "enable"/>

        
        
        
        
    
    </option>

    <!-- <solver>
        <solimp value = ".9 .95 .001 .1 6"/>
    </solver> -->
    <option timestep = ".002"/>
    <!-- <option integrator = "implicit"/> -->
    <!-- <option integrator = "implicitfast"/> -->
    <asset>
        <texture name="grid" type="2d" builtin="checker" rgb1=".1 .2 .3"
            rgb2=".2 .3 .4" width="300" height="300" mark="none"/>
        <material name="grid" texture="grid" texrepeat="1 1"
            texuniform="true" reflectance="0"/>
        <!-- <texture type="skybox" builtin="gradient" rgb1=".3 .5 .7" rgb2="0 0 0" width="32" height="512"/> -->
        <texture type="skybox" builtin="flat" rgb1=".3 .5 .7" rgb2="0 0 0" width="32" height="512"/>
    </asset>
    
    <worldbody>
        
        <!-- <geom name="floor" type="plane"  size="2 2 .1" material="grid"/> -->
        <!-- <geom name="floor" type="plane"  size="4 4 .1" material="grid" friction = "0.8 .0005 0.001"/> -->
        <geom name="floor" type="plane"  size="20 20 .1" rgba = "1 1 1 1" friction = "0.8 .0005 0.001"/>
        <!-- <geom name="floor" type="plane"  size="20 20 .1" rgba = "1 1 1 1" friction = "0 .000 0.00"/> -->


        <geom name = "obstacle" type = "box" size = ".3 .3 .3" pos = ".6 0 .15" rgba = ".5 .5 .5 1"/>

        <body name = "prop_bar" gravcomp = "0">
            <!-- <inertial mass = "1" pos = "0 0 0" diaginertia = "1 1 1"/> -->
            <!-- <contact solimp = ".9 .95 .001 .1 6"/> -->
            <!-- <joint name = "prop_free_joint" type = "free"/> -->
            <freejoint name = "prop_free_joint"/>
            <geom name = "propeller_body" type = "cylinder" size = ".005 .1" zaxis = "1 0 0" mass = "1" rgba = "1 0 0 1" friction = "0 0 0"/>
            <!-- <geom name = "propeller_body" type = "cylinder" size = ".005 .1" zaxis = "1 0 0" mass = "1" solimp = ".9 .95 .001 .1 6"/> -->
            <!-- <geom name = "propeller_body" type = "box" size = ".1 .005 .005 " mass = "1"/> -->
            
            <!-- <site name = "IMU_1_loc" pos = "0 0 0"/>  -->
            <!-- <site name = "IMU_1_loc" pos = ".1 0 0" size = ".001 .001 .001"/>  -->
            <site name = "IMU_1_loc" pos = "0 0 0" size = ".001 .001 .001"/> 
            <site name = "prop_1" pos = "-.1 0 0" size = ".001 .001 .001"/>
            <site name = "prop_2" pos = ".1 0 0" size = ".001 .001 .001"/>
            <site name = "GPS_loc" pos = ".02 0 0" size = ".001 .001 .001"/>

            <body name = "leg_1"  gravcomp = "0">
                <!-- <inertial mass = "1" pos = "0 0 0" diaginertia = "1 1 1"/> -->
                <!-- <contact solimp = ".9 .95 .001 .1 6"/> -->
                <joint name = "leg_1_joint" type = "hinge" axis = "0 1 0" pos = "0 0 0" damping = ".2"/>
                <!-- <joint name = "leg_1_joint" type = "hinge" axis = "0 1 0" pos = "0 0 0" damping = "0"/> -->

                <!-- <geom name = "leg_1_stick" type = "cylinder" fromto = "0 0 0 0 0 -.5" size = ".002" mass = "1"/> -->
                <!-- <geom name = "leg_1_stick" type = "box" fromto = "0 0 0 0 0 -.5" size = ".002 .002" mass = "1"/> -->
                <geom name = "leg_1_stick" type = "capsule" fromto = "0 0 0 0 0 -.5" size = ".002" mass = "1" friction = "0.2 000 0" rgba = "1 0 0 1"/>
                <!-- <geom name = "leg_1_stick" type = "cylinder" fromto = "0 0 0 0 0 -.5" size = ".002" mass = "1" friction = "0.2 000 0" rgba = ".25 .67 1 1"/> -->
                <!-- <geom name = "leg_1_stick" type = "capsule" fromto = "0 0 0 0 0 -.5" size = ".002" mass = "1" friction = "0 0 0" solimp = ".9 .95 .001 .9 1"/> -->
                <site name = "IMU_2_loc" pos = "0 0 -0.25" size = ".001 .001 .001"/>
                <site name = "feet" type = "sphere" size = ".03" pos = "0 0 -.5" rgba = "0 0 0 1"/>
            </body>
        </body>


    </worldbody>

    <!-- <contact>
        <exclude body1="prop_bar" body2="leg_1"/> 
    </contact> -->

    <actuator>
        <!-- <motor name = "propeller1" site = "prop_1" gear = "0 0 0 0 0 1"/>
        <motor name = "propeller2" site = "prop_2" gear = "0 0 0 0 0 1"/> -->
        <motor name = "propeller1" site = "prop_1" gear = "0 0 1 0 0 0"/>
        <motor name = "propeller2" site = "prop_2" gear = "0 0 1 0 0 0"/>
    </actuator>

    <sensor>
        <!-- <accelerometer name = "IMU_acc" site = "IMU_loc" noise = ".01"/> -->
        <accelerometer name = "IMU_1_acc" site = "IMU_1_loc" noise = ".01"/>
        <gyro name = "IMU_1_gyro" site = "IMU_1_loc" noise = ".01"/>

        <accelerometer name = "IMU_2_acc" site = "IMU_2_loc" noise = ".01"/>
        <gyro name = "IMU_2_gyro" site = "IMU_2_loc" noise = ".01"/>
        <!-- <accelerometer name = "IMU_acc" site = "IMU_loc" noise = "0"/>
        <gyro name = "IMU_gyro" site = "IMU_loc" noise = "0"/> -->

        <touch name = "feet_touch_sensor" site = "feet"/>
        <framepos name = "gps" objtype = "site" objname = "GPS_loc" noise = ".05"/>
        <framelinvel name = "gps_vel" objtype = "site" objname = "GPS_loc" noise = ".005"/>
    </sensor>
    
</mujoco>